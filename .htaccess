AddType application/x-httpd-php .htm .html



RewriteEngine On

RewriteCond %{REQUEST_FILENAME}.php -f

RewriteCond %{REQUEST_URL} !/s

RewriteRule ^(.*)$ $1\.php

RewriteEngine On

RewriteCond %{REQUEST_FILENAME}.html -f

RewriteCond %{REQUEST_URL} !/s

RewriteRule ^(.*)$ $1\.html


Options -Indexes